{
    "destination": "my.table",
    "primary_key": [
        "id"
    ],
    "delimiter": "|",
    "enclosure": "_",
    "columns": [
        "id",
        "number",
        "other_column"
    ],
    "incremental": true,
    "metadata": [
        {
            "key": "an.arbitrary.key",
            "value": "Some value"
        },
        {
            "key": "another.arbitrary.key",
            "value": "A different value"
        }
    ],
    "column_metadata": {
        "id": [
            {
                "key": "yet.another.key",
                "value": "Some other value"
            }
        ],
        "number": [
            {
                "key": "KBC.datatype.basetype",
                "value": "INTEGER"
            },
            {
                "key": "KBC.datatype.default",
                "value": 0
            }
        ],
        "other_column": [
            {
                "key": "KBC.datatype.basetype",
                "value": "BOOLEAN"
            },
            {
                "key": "KBC.datatype.default",
                "value": false
            }
        ]
    }
}
